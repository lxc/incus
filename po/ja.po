#~ "  - type={instance type}\n"
#~ "  - status={instance current lifecycle status}\n"
#~ "  - architecture={instance architecture}\n"
#~ "  - location={location name}\n"
#~ "  - ipv4={ip or CIDR}\n"
#~ "  - ipv6={ip or CIDR}\n"
